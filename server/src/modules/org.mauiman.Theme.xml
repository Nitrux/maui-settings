<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
<interface name="org.mauiman.Theme">
<method name="setAccentColor">
<arg name="accentColor" type="s" direction="in"/>
</method>
<method name="setStyleType">
<arg name="styleType" type="i" direction="in"/>
</method>
<method name="setIconTheme">
<arg name="iconTheme" type="s" direction="in"/>
</method>
<method name="setWindowControlsTheme">
<arg name="windowControlsTheme" type="s" direction="in"/>
</method>

<property name="accentColor" type="s" access="read"/>
<property name="styleType" type="i" access="read"/>
<property name="iconTheme" type="s" access="read"/>
<property name="windowControlsTheme" type="s" access="read"/>

<signal name="accentColorChanged">
<arg type="s"/>
</signal>
<signal name="styleTypeChanged">
<arg type="i"/>
</signal>
<signal name="iconThemeChanged">
<arg type="s"/>
</signal>
<signal name="windowControlsThemeChanged">
<arg type="s"/>
</signal>
</interface>
</node>
